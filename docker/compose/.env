COMPOSE_PROJECT_NAME=cruud_stack
DB_HOST=mongodb
DB_NAME=cruud
DB_USERNAME=admin
DB_PASSWORD=password
DB_PORT=27017
BASICAUTH=false
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
