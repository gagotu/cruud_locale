SERVER_PORT=8090
HOST_DOCUMENTS_DIR=${USERPROFILE:-${HOME}}/Documents
MONGO_DATA_SUBDIR=DockerVolumes/mongo
APP_DATA_SUBDIR=DockerVolumes/cruud
